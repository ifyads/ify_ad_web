


CREATE TABLE shortener (short_code VARCHAR(10) PRIMARY KEY, long_url VARCHAR(2048) NOT NULL, ip VARCHAR(35) , datetime DATETIME NOT NULL DEFAULT CURRENT_TIMESTAMP);


CREATE TABLE advisement_log (ip VARCHAR(45), device VARCHAR(50) ,operating VARCHAR(100), datetime DATETIME DEFAULT CURRENT_TIMESTAMP);



CREATE TABLE admin (username VARCHAR(8) NOT NULL UNIQUE, password VARCHAR(63) NOT NULL,salt VARCHAR(32) NOT NULL); -- len


