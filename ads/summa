CREATE TABLE admin_log (  username VARCHAR(255) NOT NULL UNIQUE, hashed_password CHAR(60) NOT NULL, last_login DATETIME DEFAULT CURRENT_TIMESTAMP  );


CREATE TABLE admins (failed_login_attempts INT DEFAULT 0, account_locked BOOLEAN DEFAULT FALSE);



